{
    "name": "theferrer/bingo",
    "license": "MIT",
    "type": "project",
    "description": "Bingo Kata",
    "require": {
        "php": "^7.3",
        "symfony/console": "^4.2",
        "nubs/random-name-generator": "^2.1",
        "lambdish/phunctional": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "phpunit": "phpunit",
        "test": [
            "@phpunit"
        ]
    },
    "autoload": {
        "psr-4": {
            "": [
                "src"
            ],
            "Bingo\\": "app/src"
        },
        "files": [
            "src/utils.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bingo\\Test\\": "tests/"
        }
    }
}
